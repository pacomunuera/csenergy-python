@incollection{zarzaOVERALLTHERMALLOSS1988,
  title = {{{OVERALL THERMAL LOSS COEFFICIENT AND INCIDENT ANGLE MODIFIER FOR AN ACUREX}} 3001 {{DCS}}: {{APPLICATION TO THE CONTROL SYSTEM}}},
  shorttitle = {{{OVERALL THERMAL LOSS COEFFICIENT AND INCIDENT ANGLE MODIFIER FOR AN ACUREX}} 3001 {{DCS}}},
